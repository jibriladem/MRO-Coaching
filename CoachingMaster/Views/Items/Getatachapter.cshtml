<section class="content">
    <!-- Default box -->
    <div class="card">
        <div class="card-body">
            <div class="box-body table-responsive no-padding">
                <table id="TableControl" class="table table-hover">
                    <thead>
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.ATA_Chapter)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Chapter_Title)
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        @{
                            if (Model != null)
                            {
                                foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.ATA_Chapter)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Chapter_Title)
                                        </td>
                                    </tr>            
                                    }
                                }
                             }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</section>
