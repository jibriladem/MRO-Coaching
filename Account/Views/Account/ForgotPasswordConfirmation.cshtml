@{
    ViewBag.Title = "Forgot Password Confirmation";
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>PSETSE | Confirmation</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    @*<environment include="Production">*@
        <!-- Font Awesome -->
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
        <!-- Ionicons -->
        <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
        <!-- Theme style -->
        <link rel="stylesheet" href="~/AdminLTE/dist/css/adminlte.min.css" />
        <!-- iCheck -->
        <link rel="stylesheet" href="~/AdminLTE/plugins/iCheck/square/blue.css">
        <!-- Google Font: Source Sans Pro -->
        <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
    @*</environment>*@
</head>

<body class="hold-transition register-page">

    <div class="login-box">
        <div class="login-logo">
            <a href="#"><b>ET MRO Coatching </b> Forget Password Confirmation</a>
        </div>
        <div class="login-box-body">
            <p class="login-box-msg">Please check your email to reset your password...</p>
        </div>
    </div>

    <!-- /.login-box -->
    @*<environment include="Production">*@
        <!-- jQuery -->
        <script src="~/AdminLTE/plugins/jquery/jquery.min.js"></script>
        <!-- Bootstrap 4 -->
        <script src="~/AdminLTE/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
        <!-- SlimScroll -->
        <script src="~/AdminLTE/plugins/iCheck/icheck.min.js"></script>
        <!-- FastClick -->
        <script>
            $(function () {
                $('input').iCheck({
                    checkboxClass: 'icheckbox_square-blue',
                    radioClass: 'iradio_square-blue',
                    increaseArea: '20%' // optional
                })
            })
        </script>
    @*</environment>*@
</body>

</html>




	
